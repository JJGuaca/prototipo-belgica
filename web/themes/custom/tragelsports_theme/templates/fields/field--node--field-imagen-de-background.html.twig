{% set imgUrl = '' %}
{% for item in items %}
  {% set imgUrl = item.content|render|striptags|trim %}
{% endfor %}
<div class="d-none" id="oculto" data="{{imgUrl}}"></div>
<script>
  document.addEventListener("DOMContentLoaded", function() {
    let main = document.querySelector('div.main');
    let item = document.getElementById('oculto');
    let dato = item.getAttribute('data');
    main.style.backgroundImage = 'url('+dato+')';
  });
</script>